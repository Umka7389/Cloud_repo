<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="localStorageItemCell" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      prefHeight="40" prefWidth="430.0" stylesheets="styles.css">
    <HBox fx:id="filesDescriptionLocal">
        <Label fx:id="fileIcon" maxHeight="20" prefWidth="60"/>
        <Label fx:id="localItemName" prefWidth="240"/>
        <Label fx:id="localItemSize" prefWidth="110"/>
        <Label fx:id="localItemModified" prefWidth="150"/>
    </HBox>
</VBox>
